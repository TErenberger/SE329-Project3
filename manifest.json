{
    "name": "Tab Manager",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Let's you very easy and fast handle your tabs!",
    "permissions": ["tabs", "storage"],
    "options_page": "options.html",
    "browser_action": {
        "default_icon": "browser_action.png",
        "default_popup": "popup.html"
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+A",
                "mac": "Command+Shift+A",
                "linux": "Ctrl+Shift+A"
            }
        }
    }

}